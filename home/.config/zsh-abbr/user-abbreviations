abbr -g "git b"="git branch"
abbr -g "git c"="git commit"
abbr -g "git co"="git checkout"
abbr -g "git discard"="git checkout ."
abbr -g "git graph"="git log --graph --oneline --decorate"
abbr -g "git save"="git commit -m"
abbr -g "git st"="git status"
abbr -g "git sw"="git switch"
abbr -g "git undo"="git reset --mixed HEAD~"
abbr -g "git unstage"="git reset --mixed HEAD"
abbr "c"="code"
abbr "g"="git"
abbr "n"="npm"
abbr "p"="pnpm"
abbr "y"="yarn"
